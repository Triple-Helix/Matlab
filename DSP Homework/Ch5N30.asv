n = [-20:1:20];
ha = [1,-2,3,-4,0,4,-3,2,-1];
hc = [1/9,2/9,3/9,2/9,1/9];
hd = [1,-1.1756,1];
x = sin(.1.*pi.*n)+1/3.*sin(.3.*pi.*n)+1/5.*sin(.5.*pi.*n);

ya = conv(ha,x);
yc = conv(hc,x);
yd = conv(hd,x);

figure(1)
stem(n,ya,'fill');
hold off
figure(2)
stem(n,yc,'fill');
hold off
figure(3)
stem(n,yd,'fill');
hold off